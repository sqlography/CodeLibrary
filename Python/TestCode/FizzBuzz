##Playing with fizzbuzz
    
#sol 1
def fizzbuzz() -> None:
    for i in range(1, 101, 1):
        if i % 3 == 0 and i % 5 == 0:
            print(f"{i} FizzBuzz")
        elif i % 5 == 0:
            print(f"{i} Buzz")
        elif i % 3 == 0:
            print(f"{i} Fizz")
 
def main() -> None:
    fizzbuzz()   
    
if __name__ == '__main__':
    main()
    
